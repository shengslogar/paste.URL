{
  "manifest_version": 2,
  "name": "paste.URL",
  "short_name": "paste.URL",
  "description": "Paste copied URL from clipboard and navigate in one click.",
  "version": "0.0.1",
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "assets/icons/icon128.png",
    "default_title": "Paste and go"
  },
  "background": {
    "scripts": [
      "assets/scripts/browser-action.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "clipboardRead"
  ]
}
